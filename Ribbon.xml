<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load" loadImage="Ribbon_LoadImage">
	<ribbon>
		<tabs>
			<tab idMso="TabMail">
				<group id="TypicalReplyTabMailGroup" label="TypicalReply" insertAfterMso="GroupMailRespond">
					<gallery id="TypicalReplyTabMailGroupGallery" getLabel="GetLabel" image="logo.png" showImage="1"></gallery>
				</group>
			</tab>
			<tab idMso="TabReadMessage">
				<group id="TypicalReplyTabReadMessageGroup" label="TypicalReply" insertAfterMso="GroupRespond">
					<gallery id="TypicalReplyTabReadMessageGroupGallery" getLabel="GetLabel" image="logo.png" showImage="1"></gallery>
				</group>
			</tab>
		</tabs>
	</ribbon>
	<contextMenus>
		<contextMenu idMso="ContextMenuMailItem">
			<gallery id="TypicalReplyContextMenuGallery" getLabel="GetLabel" insertAfterMso="Forward" image="logo.png" showImage="1"></gallery>
		</contextMenu>
	</contextMenus>
</customUI>

